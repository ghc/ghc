\end{code}

%************************************************************************
%*									*
\subsubsection[ticky-counters]{The accumulators (extern decls)}
%*									*
%************************************************************************

\begin{code}
extern I_ ALLOC_HEAP_ctr;
extern I_ ALLOC_HEAP_tot;

extern PP_ max_SpA;
extern P_  max_SpB;

extern I_ A_STK_STUB_ctr;
/* not used at all?
extern I_ A_STK_REUSE_ctr;
