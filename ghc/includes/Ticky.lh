				UPD_HISTO(CON_IN_NEW,n)
#define UPD_PAP_IN_NEW(n)	UPD_PAP_IN_NEW_ctr++ ; \
				UPD_HISTO(PAP_IN_NEW,n)
/* ToDo: UPD_NEW_COPY_ctr, as below */

#define UPD_CON_IN_PLACE(n)	UPD_CON_IN_PLACE_ctr++ ; \
