name: q
version: 0.1
build-type: Simple
cabal-version: >= 1.2

library
  exposed-modules: Q
  build-depends: base, filepath

executable buildable-false
  main-is: Main.hs
  buildable: False

test-suite solver-disabled
  type: exitcode-stdio-1.0
  main-is: Test.hs
  build-depends: a-package-that-does-not-exist

benchmark user-disabled
  type: exitcode-stdio-1.0
  main-is: Test.hs

