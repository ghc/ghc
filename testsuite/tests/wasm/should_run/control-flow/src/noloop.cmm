noloop (bits32 n) {
  A:
    foreign "C" A();
    if (n > 0) goto B;
  G:
    foreign "C" G();
    if (n > 7) goto F;
  E:
    foreign "C" E();
    return (999);
  B: 
    foreign "C" B();
    if (n > 2) goto C;
  F:
    foreign "C" F();
    goto D;
  C:
    foreign "C" C();
  D:
    foreign "C" D();
    goto E;
}
