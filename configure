HaskellCPPArgs="$HaskellCPPArgs $HS_CPP_ARGS"





# --with-js-cpp/--with-js-cpp-flags


# Check whether --with-js-cpp was given.
if test ${with_js_cpp+y}
then :
  withval=$with_js_cpp;
  if test "$target_cpu" = "javascript" ; then
    JS_CPP_CMD=$withval
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Request to use $withval will be ignored" >&5
printf "%s\n" "$as_me: WARNING: Request to use $withval will be ignored" >&2;}
    JS_CPP_CMD=$CC
  fi

else case e in #(
  e)
  if test "$target_cpu" = "javascript" ; then
    if "$CC" --version 2> /dev/null | grep "emcc" > /dev/null 2>&1;
    then
      JS_CPP_CMD=$CC
    else
      as_fn_error $? "configure can not recognize your CPP program for JavaScript preprocessing, you need to set --with-js-cpp=ARG explicitly" "$LINENO" 5
      JS_CPP_CMD=""
    fi
  else
    JS_CPP_CMD=$CC
  fi

 ;;
esac
fi



# Check whether --with-js-cpp-flags was given.
if test ${with_js_cpp_flags+y}
then :
  withval=$with_js_cpp_flags;
  if test "$target_cpu" = "javascript" ; then
    JS_CPP_ARGS=$withval
    USER_JS_CPP_ARGS=$withval
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Request to use $withval will be ignored" >&5
printf "%s\n" "$as_me: WARNING: Request to use $withval will be ignored" >&2;}
  fi

else case e in #(
  e)
  # See: https://gcc.gnu.org/onlinedocs/gcc/Preprocessor-Options.html#index-CC
  # See: https://clang.llvm.org/docs/ClangCommandLineReference.html#cmdoption-clang-CC
  # Emscripten supports -C and -CC options same as GCC and CLang
  # We have to use -nostdinc to prevent adding copyright headers in gcc output.
  # This issue is known and discussed here: https://gcc.gnu.org/bugzilla/show_bug.cgi?id=59566
  JS_CPP_ARGS="-E -CC -Wno-unicode -nostdinc"

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether $JS_CPP_CMD preprocessor supports flags $JS_CPP_ARGS" >&5
printf %s "checking whether $JS_CPP_CMD preprocessor supports flags $JS_CPP_ARGS... " >&6; }

  cat <<EOT >> ./conftest.js
#define DEF_TEST
#ifdef DEF_TEST
// 1
#endif
EOT

  "$JS_CPP_CMD" $JS_CPP_ARGS -P -x assembler-with-cpp ./conftest.js -o ./conftest.pp.js > conftest.txt 2>&1

  if test $? -eq 0 ; then
    # We trim \n due of difference between Emscripten CC and clang.
    # Clang version preserves whitespace lines, but emcc removes.
    if test "$(cat ./conftest.pp.js | tr -d '\n')" = "// 1" ; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
    else
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
      echo "processed result:"
      cat ./conftest.pp.js
      as_fn_error $? "$JS_CPP_CMD must produce correct output $JS_CPP_ARGS" "$LINENO" 5
    fi
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
    echo "command execution output:"
    cat ./conftest.txt
    as_fn_error $? "$JS_CPP_CMD must support the flags $JS_CPP_ARGS" "$LINENO" 5
  fi

  rm -f conftest.js conftest.pp.js conftest.txt

 ;;
esac
fi


JavaScriptCPPCmd=$JS_CPP_CMD
JavaScriptCPPArgs="$JavaScriptCPPArgs $JS_CPP_ARGS"





