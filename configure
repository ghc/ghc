        LDFLAGS="$LDFLAGS -m64"
        CPPFLAGS="$CPPFLAGS -m64"
        if test "$fp_cv_gnu_ld" = "yes"; then
            IGNORE_LINKER_LD_FLAGS="$IGNORE_LINKER_LD_FLAGS -m64"
        else
            IGNORE_LINKER_LD_FLAGS="$IGNORE_LINKER_LD_FLAGS -64"
        fi
        ;;
