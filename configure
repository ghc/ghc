    if test x"$LLC" != x
then :

        PROG_VERSION=`$LLC --version | sed -n -e 's/.*version \(\([0-9][0-9]*\.\)\([0-9][0-9]*\.\)*[0-9][0-9]*\).*/\1/gp'`
        if test x"$PROG_VERSION" = x
then :
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
           LLC=""
