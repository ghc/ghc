  withval=$with_intree_gmp; case x"$withval" in #(
  x"yes") :
    GMP_FORCE_INTREE=YES ;; #(
  x"no") :
    GMP_FORCE_INTREE=NO ;; #(
  *) :
    as_fn_error $? "invalid value: \"$withval\" is not \"yes\" or \"no\"" "$LINENO" 5 ;;
esac

else case e in #(
  e) GMP_FORCE_INTREE=NO ;;
esac
fi



